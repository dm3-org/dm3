        "@types/jest": "^28.1.1",
        "@types/node": "^16.7.13",
        "@types/uuid": "^8.3.4",
        "jest": "^28.1.1",
        "ts-jest": "^28.0.4",
        "ts-json-schema-generator": "^0.98.0"
